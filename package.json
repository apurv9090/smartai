    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "classix": "^2.2.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.462.0",
    "morgan": "^1.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
